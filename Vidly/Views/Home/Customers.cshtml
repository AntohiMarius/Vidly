@model IEnumerable<Vidly.Models.Customer>

@{
    ViewBag.Title = "Customers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>

<table class="table">

@foreach (var item in Model) {
    <tr>
        <td>
            <a href="#">@Html.DisplayFor(modelItem => item.Name)</a>
        </td>
    </tr>
}

</table>
